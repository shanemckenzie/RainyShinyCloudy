target 'RainyShinyCloudy' do

  source 'https://github.com/CocoaPods/Specs.git'
  platform :ios, '10.0'
  use_frameworks!
  pod ‘Alamofire’, '~> 4.0'


  target 'RainyShinyCloudyTests' do
    
    # Pods for testing
    pod ‘Alamofire’, '~> 4.0'
  end

  target 'RainyShinyCloudyUITests' do
    inherit! :search_paths
    pod ‘Alamofire’, '~> 4.0'

  end

end
